<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is not required if you don't need any extra configuration. -->
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="         http://java.sun.com/xml/ns/javaee         http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<!-- This descriptor activates the JSF 2.0 Servlet Write your navigation 
		rules here. You are encouraged to use CDI for creating @Named managed beans. -->
	<managed-bean>
		<managed-bean-name>userBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.User</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.util.Login</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>logoutBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.util.Logout</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.util.Register</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<!-- ManagedBeans relatifs Ã  l'arborescence de l'examen -->
	<managed-bean>
		<managed-bean-name>examenBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>enonceBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>exerciceBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>questionBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>caseACocherBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.Caseacocher</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>caseBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.Caseacocher.Case</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>choixMultiplesBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.ChoixMultiples</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>choixBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.ChoixMultiples.Choix</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>codeOuTexteBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.CodeOuTexte</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>segmentBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.CodeOuTexte.Segment</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>protocoleBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Enonce.Exercice.Question.Protocole</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>informationBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Information</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>destinationBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Information.Destination</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>enTeteBean</managed-bean-name>
		<managed-bean-class>fr.emn.examination.model.examen.Examen.Information.EnTete</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{loginBean.login}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/restricted/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>register</from-outcome>
			<to-view-id>/register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/register.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{registerBean.register}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
